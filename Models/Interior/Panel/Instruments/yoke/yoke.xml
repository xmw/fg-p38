<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>yoke.ac</path>

  <animation>
    <object-name>base</object-name>
    <object-name>yoke</object-name>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>yoke</object-name>
    <property>controls/flight/aileron</property>
    <factor> -45 </factor>
    <center>
      <x-m>  0.03496 </x-m>
      <y-m> -0.25999 </y-m>
      <z-m>  0.42273 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <name>AllYoke</name>
    <object-name>base</object-name>
    <object-name>yoke</object-name>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>AllYoke</object-name>
    <property>controls/flight/elevator</property>
    <factor> -5 </factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
</PropertyList>
